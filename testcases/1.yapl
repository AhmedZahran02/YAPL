int x = 5, y = 2;

// mathematical operations 
print( x + y );
print( x - y );
print( x * y );
print( x / y );
print( x % y );
print( pow(x, y) );
print( sqrt(x) );

// increments
int w = x;
print(w++ + " " + w);
print(++w + " " + w);
print(--w + " " + w);
print(w-- + " " + w);

// bitwise operations
print( x & y );
print( x | y );
print( x ^ y );
print( x << y );
print( x >> y );
print( ~x );
print( ~y );